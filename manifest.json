{
  "manifest_version": 3,
  "name": "StyleInjector",
  "version": "1.0",
  "description": "Add custom css to sites.",
  "permissions": [
    "storage",     
    "activeTab",   
    "scripting",   
    "tabs"         
  ],
  "host_permissions": [
    "<all_urls>" 
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png" 
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "icon.png" 
  }
}